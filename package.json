{
  "name": "template-backend-nodejs",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "license": "ISC",
  "packageManager": "pnpm@10.26.2",
  "author": "",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0",
    "npm": "use-pnpm-please"
  },
  "scripts": {
    "build": "run-s ci:generate build:tsup",
    "start:tsx:watch": "tsx watch --env-file=.env --no-deprecation src/server.ts",
    "start:dev": "run-s start:tsx:watch",
    "start:prod": "node --enable-source-maps --env-file=.env dist/server.js",
    "start:prod:docker": "node --enable-source-maps dist/server.js",
    "build:tsc": "tsc",
    "build:tsgo": "tsgo",
    "build:tsup": "tsup",
    "lint:src:biome": "biome lint --write src",
    "lint:prisma:biome": "biome lint --write prisma",
    "check:src:biome": "biome check --write src",
    "check:prisma:biome": "biome check --write prisma",
    "db:generate": "prisma generate",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:dev:default": "prisma migrate dev --name creating_database_models",
    "db:migrate:reset": "prisma migrate reset --force",
    "db:seed": "prisma db seed",
    "db:reset": "run-s db:generate db:migrate:reset db:seed",
    "db:migrate-deploy": "prisma migrate deploy",
    "db:deploy": "run-s db:migrate-deploy db:generate db:seed",
    "ci:generate": "run-p db:generate",
    "ci:lint": "run-p check:prisma:biome check:src:biome",
    "ci:format": "run-p check:prisma:biome check:src:biome",
    "ci:build": "run-s ci:generate build:tsgo build:tsup",
    "sentry:upload": "sentry-cli sourcemaps inject ./dist && sentry-cli sourcemaps upload ./dist"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@commitlint/config-conventional": "20.3.0",
    "@commitlint/types": "20.2.0",
    "@sentry/cli": "3.0.1",
    "@swc/core": "1.15.10",
    "@types/jsonwebtoken": "9.0.10",
    "@types/ms": "2.1.0",
    "@types/node": "25.0.9",
    "@types/nodemailer": "7.0.5",
    "@types/pg": "8.16.0",
    "@typescript/native-preview": "7.0.0-dev.20260105.1",
    "commitlint": "20.3.0",
    "es-errors": "1.3.0",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "npm-run-all": "4.1.5",
    "pino-pretty": "13.1.3",
    "prisma": "7.2.0",
    "ts-node": "10.9.2",
    "tsup": "8.5.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "11.2.0",
    "@fastify/jwt": "10.0.0",
    "@fastify/multipart": "9.3.0",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/static": "9.0.0",
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "@sentry/node": "10.32.1",
    "@sentry/profiling-node": "10.32.1",
    "bcryptjs": "3.0.3",
    "cpf-cnpj-validator": "1.0.3",
    "cron-validator": "1.4.0",
    "doi-utils": "2.0.6",
    "dotenv": "17.2.3",
    "fastify": "5.6.2",
    "fastify-cookie": "5.7.0",
    "ioredis": "5.9.2",
    "json2csv": "6.0.0-alpha.2",
    "jsonwebtoken": "9.0.3",
    "ms": "2.1.3",
    "node-cron": "4.2.1",
    "nodemailer": "7.0.12",
    "pg": "8.17.1",
    "pino": "10.2.1",
    "reflect-metadata": "0.2.2",
    "sharp": "0.34.5",
    "slash": "5.1.0",
    "tsyringe": "4.10.0",
    "uuid": "13.0.0",
    "zod": "4.3.5"
  }
}
